}

/* Style for menu widget  */
#menuWidget {
	background-color: #353535;
}

/* Style for QPushButton in menu widget  */
#menuWidget QPushButton {
	color: #000;
	text-align: left;
	padding-left: 15px;
	border: none;
	background-color: #fff;
	font-size: 14px;
	border: 5px solid #353535;
	height: 25px;
	border-radius: 8px;
}

/* Style for QPushButton*/
QPushButton {
	text-align: center;
	padding-left: 15px;
	border: none;
	background-color: #fff;
	font-size: 14px;
	border: 5px solid #353535;
	height: 25px;
	border-radius: 8px;
}

#menuWidget QPushButton:hover {
	background-color: #e3e3e3;
}


#menuWidget QPushButton:checked {
	background-color: #0055ff;
	color: #fff;
}

/* Style for main widget */
#stackedWidget {
	background-color: #f4f5f8;
}

/* Style for QLabel in main widget */
#stackedWidget QLabel {
	font-size: 14px;
	font-family: &quot;Segoe UI Semibold&quot;;
}

/* Style for QLineEdit and QSpinBox in main widget */
#stackedWidget QLineEdit, #stackedWidget QSpinBox {
	border: 1px solid #353535;
	border-radius: 3px;
	padding: 5px 10px;
	font-size: 12px;
}

/* style for  QSpinBox*/
#stackedWidget QSpinBox::up-arrow {
	image: url(:/icon/icon/arrow-146-48.ico);
    width: 12px;
    height: 12px;
}

#stackedWidget QSpinBox::down-arrow {
	image: url(:/icon/icon/arrow-208-48.ico);
    width: 12px;
    height: 12px;
}

#stackedWidget QSpinBox::down-button, 
#stackedWidget QSpinBox::up-button {
	border: none;
	width: 30px;
}

#stackedWidget QSpinBox::down-button:hover, 
#stackedWidget QSpinBox::up-button:hover {
	background-color: rgb(176, 176, 176);
}

#stackedWidget QSpinBox::down-button:pressed, 
#stackedWidget QSpinBox::up-button:pressed {
	background-color: rgb(78, 88, 121);
}

/* Style for QPushButton in main widget */
#stackedWidget QPushButton {
	border: none;
	font-size: 16px;
	border-radius: 3px;
	color: #fff;
	padding: 5px 10px;
}

#stackedWidget QPushButton:pressed {
    padding-left: 20px;
}

/* Style for sepcial QPushButton in main widget */
#confEditBtn,
#generateCopyBtn, 
#generateRestBtn,
#showSearchBtn
{
	background-color: #3554d1;
}

#confEditBtn:hover, #confEditBtn:pressed, 
#generateCopyBtn:hover,  #generateCopyBtn:pressed, 
#generateRestBtn:hover,  #generateRestBtn:pressed, 
#showSearchBtn:hover,  #showSearchBtn:pressed 
{
	background-color: #0000ff;
}

#confSaveBtn,
#generateSaveBtn, 
#generateCreateBtn
{
    background-color: #00aa7f;
}

#confSaveBtn:hover, #confSaveBtn:pressed, 
#generateSaveBtn:hover, #generateSaveBtn:pressed,
#generateCreateBtn:hover, #generateCreateBtn:pressed 
{
	background-color: #00aa00;
}

#confRefreshBtn,
 #showRefreshBtn
{
    background-color: #E59866;
}


#confRefreshBtn:hover, #confRefreshBtn:pressed, 
#showRefreshBtn:hover, #showRefreshBtn:pressed
{
	background-color: #DC7633;
}

/* Style for QCheckBox in main widget */
QCheckBox {
	spacing: 10px;
	font-size: 14px;
}

QCheckBox::indicator {
    width: 15px;
    height: 15px;
}

QCheckBox::indicator:unchecked {
	image: url(:/icon/icon/checkbox_unchecked.ico);
}

QCheckBox::indicator:checked {
	image: url(:/icon/icon/checkbox_checked.ico);
}


/* Style for QFrame */
#resultFrame {
	border-radius: 5px;
	background-color: #fff;
}

/* Style for QTableWidget */
#tableWidget QHeaderView, #tableWidget   {
	border:0px;
	background-color:  #fff;
	border-radius:5px;
	text-align:left;
}

#tableWidget QHeaderView::section{
	font-family:&quot;Times New Roman&quot;, &quot;Times&quot;, &quot;serif&quot;;
	font-size: 12px;
	font-weight: bold;
	text-align:left;
	border-radius:14px;
	border-bottom:1px solid #353535;
	border-top:1px solid #353535;
	padding-left: 10px;
}

#tableWidget::item:selected {
	background-color: #55aaff;
}

#tableWidget::item{
	border-bottom:1px solid #d0c6ff;
	padding-left: 10px;
	color: black;
}

